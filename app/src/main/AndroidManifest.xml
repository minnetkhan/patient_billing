<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.patientbilling">

    <application
        android:name=".FirstTime"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".WardDashboard"></activity>
        <activity android:name=".TreatmentDashboard" />
        <activity android:name=".DoctorsDashboard" />
        <activity android:name=".MainDashBoard" />
        <activity android:name=".AddPatient" />
        <activity android:name=".AllPatientDetails" />
        <activity android:name=".PatientDetails" />
        <activity android:name=".LoggedInScreen" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>